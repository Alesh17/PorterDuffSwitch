<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PorterDuffSwitch">
        <attr name="pds_textSize" format="dimension" />
        <attr name="pds_fontFamily" format="string" />
        <attr name="pds_indent" format="dimension" />
        <attr name="pds_textLeft" format="string" />
        <attr name="pds_textRight" format="string" />
        <attr name="pds_colorActive" format="color" />
        <attr name="pds_colorInactive" format="color" />
        <attr name="pds_defaultState">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
</resources>